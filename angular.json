{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"k-chess": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"aot": true,
						"assets": [
							"src/favicon.ico",
							"src/assets",
							"src/config.json",
							"src/frame.html",
							"src/manifest.webmanifest",
							"src/robots.txt",
							{
								"glob": "**/*",
								"input": "./node_modules/stockfish.js/",
								"output": "./assets/stockfish.js/"
							},
							{
								"glob": "app.manifest.json",
								"input": "./",
								"output": "./"
							},
							{
								"glob": "**/*",
								"input": "node_modules/@kognifai/design-system/styles/img/components/",
								"output": "./assets/img/components/"
							},
							{
								"glob": "**/*",
								"input": "node_modules/@kognifai/design-system/styles/img/gfx/",
								"output": "./assets/img/gfx/"
							},
							{
								"glob": "**/*",
								"input": "node_modules/@kognifai/design-system/styles/img/icons/",
								"output": "./assets/img/icons/"
							},
							{
								"glob": "ai.svg",
								"input": "node_modules/@kognifai/design-system/styles/img/icons/general/",
								"output": "./assets/images/"
							},
							{
								"glob": "oidc-client.min.js",
								"input": "../../node_modules/oidc-client/dist/",
								"output": "./"
							},
							{
								"glob": "oidc-client.min.js",
								"input": "node_modules/oidc-client/dist/",
								"output": "./"
							},
							{
								"glob": "*.js",
								"input": "../../node_modules/@kognifai/poseidon-authredirect/dist/",
								"output": "./"
							},
							{
								"glob": "*.js",
								"input": "node_modules/@kognifai/poseidon-authredirect/dist/",
								"output": "./"
							},
							{
								"glob": "perform-authredirect.js",
								"input": "src/scripts/",
								"output": "./"
							},
							{
								"glob": "perform-silentsignin.js",
								"input": "src/scripts/",
								"output": "./"
							}
						],
						"styles": [
							{
								"input": "node_modules/@kognifai/design-system/styles/css/day-theme.css",
								"lazy": true,
								"bundleName": "kx-day-theme"
							},
							{
								"input": "node_modules/@kognifai/design-system/styles/css/dusk-theme.css",
								"lazy": true,
								"bundleName": "kx-dusk-theme"
							},
							{
								"input": "src/styles/app-day-theme.css",
								"lazy": true,
								"bundleName": "app-day-theme"
							},
							{
								"input": "src/styles/app-dusk-theme.css",
								"lazy": true,
								"bundleName": "app-dusk-theme"
							},
							"src/styles.scss",
							"node_modules/ngx-toastr/toastr.css"
						],
						"extractCss": true,
						"scripts": [
							"node_modules/@kognifai/design-system/styles/scripts/kx/navbar.js"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"src/app/styles"
							]
						}
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								},
								{
									"replace": "app.manifest.json",
									"with": "app.manifest.prod.json"
								},
								{
									"replace": "src/config.json",
									"with": "src/config.prod.json"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"serviceWorker": true,
							"ngswConfigPath": "ngsw-config.json"
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "k-chess:build",
						"port": 4301
					},
					"configurations": {
						"production": {
							"browserTarget": "k-chess:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "k-chess:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "karma.conf.js",
						"assets": [
							"src/favicon.ico",
							"src/assets",
							"src/manifest.webmanifest"
						],
						"styles": [
							"src/styles.scss"
						],
						"scripts": [],
						"stylePreprocessorOptions": {
							"includePaths": [
								"src/app/styles"
							]
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"tsconfig.app.json",
							"tsconfig.spec.json",
							"e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "e2e/protractor.conf.js",
						"devServerTarget": "k-chess:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "k-chess:serve:production"
						}
					}
				}
			}
		}
	},
	"defaultProject": "k-chess"
}